{
  "name": "otiuming",
  "version": "1.0.0",
  "keywords": [],
  "license": "ISC",
  "main": "index.js",
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.2.1",
    "http-server": "^14.1.1",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitest/coverage-v8": "^3.0.8",
    "eslint": "^9.22.0",
    "nx": "^20.5.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8",
    "standard-version": "^9.5.0",
    "sync-workspace-version": "^0.3.0"
  },
  
  "scripts": {
    "build": "nx build",
    "build-all": "nx run-many --target=build --all",
    "build-apps": "nx run-many --target=build --projects apps/**/*",
    "build-libs": "nx run-many --target=build --projects libs/**/*",
    "coverage": "nx coverage",
    "coverage-all": "nx run-many --target=coverage --all",
    "dev": "nx dev",
    "dev-all": "nx run-many --target=dev --all",
    "nx": "nx",
    "test": "nx test",
    "test-all": "nx run-many --target=test --all",
    "lint": "nx lint",
    "lint-all": "nx run-many --target=lint --all",
    "release": "standard-version && pnpm run sync-versions",
    "release:minor": "standard-version --release-as minor && pnpm run sync-versions",
    "release:major": "standard-version --release-as major && pnpm run sync-versions",
    "release:patch": "standard-version --release-as patch && pnpm run sync-versions",
    "sync-versions": "sync-workspace-version"
  }
}
