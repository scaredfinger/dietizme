{
  "name": "@otiuming/domain-omnidata-types",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.lib.json",
    "lint": "eslint",
    "update-schema": "graphql-codegen --config graphql-codegen.yaml",
    "export-schema": "gq https://local.hasura.nhost.run/v1/graphql -H 'X-Hasura-Admin-Secret: nhost-admin-secret' --introspect > schema.graphql",
    "env": "env"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@otiuming/base": "workspace:*",
    "graphql": "^16.10.0",
    "graphqurl": "^2.0.0"
  }
}
